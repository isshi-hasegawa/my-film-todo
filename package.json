{
  "name": "my-film-todo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "prepare": "simple-git-hooks > /dev/null"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix --quiet"
    ],
    "{public,src}/**/*.{html,gql,graphql,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "2.0.9",
    "@chakra-ui/react": "2.3.1",
    "@emotion/react": "11.10.0",
    "@emotion/styled": "11.10.0",
    "axios": "^0.27.2",
    "framer-motion": "7.2.1",
    "moviedb-promise": "^3.1.18",
    "next": "12.2.5",
    "next-auth": "4.10.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@types/eslint": "8.4.6",
    "@types/jest": "28.1.8",
    "@types/node": "18.7.10",
    "@types/prettier": "2.7.0",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "babel-jest": "28.1.3",
    "eslint": "8.23.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "8.5.0",
    "jest": "28.1.3",
    "jest-environment-jsdom": "29.0.2",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "simple-git-hooks": "2.8.0",
    "typescript": "4.7.4"
  }
}
